{
  "name": "react-base",
  "version": "1.0.0",
  "description": "It will be base for react app, having common features",
  "scripts": {
    "lint": "tsc && eslint --ext .js,.ts,.tsx src",
    "format": "npm run prettier -- --write",
    "prettier": "prettier \"**/*.+(js|jsx|json|css|scss|ts|tsx|md|mdx)\"",
    "validate": "npm run lint && npm run test && npm run prettier -- --list-different",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "webpack-dev-server --open --config webpack.dev.ts",
    "build": "webpack --mode production --config webpack.prod.ts"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix --max-warnings 5",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "dependencies": {
    "@hot-loader/react-dom": "^16.11.0",
    "lodash": "^4.17.15",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-hot-loader": "^4.12.19",
    "react-redux": "^7.1.3",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "redux-saga": "^1.1.3"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@types/git-revision-webpack-plugin": "^3.0.0",
    "@types/html-webpack-plugin": "^3.2.2",
    "@types/mini-css-extract-plugin": "^0.9.0",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/tape": "^4.2.34",
    "@types/webpack": "^4.41.6",
    "@typescript-eslint/eslint-plugin": "^2.19.0",
    "@typescript-eslint/parser": "^2.19.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-eslint": "^10.0.3",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-config-react-app": "^5.2.0",
    "eslint-import-resolver-webpack": "^0.12.1",
    "eslint-plugin-flowtype": "^4.6.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "file-loader": "^5.0.2",
    "git-revision-webpack-plugin": "^3.0.4",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.2.1",
    "lint-staged": "^10.0.7",
    "mini-css-extract-plugin": "^0.9.0",
    "prettier": "^1.19.1",
    "tape": "^4.13.0",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/avaniklsjoshi/React-base.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/avaniklsjoshi/React-base/issues"
  },
  "homepage": "https://github.com/avaniklsjoshi/React-base#readme"
}
