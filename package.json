{
  "name": "react-base",
  "version": "1.0.0",
  "description": "It will be base for react app, having common features",
  "scripts": {
    "lint": "tsc && eslint --no-ignore --ext .js,.ts,.tsx src",
    "format": "npm run prettier -- --write",
    "prettier": "prettier \"**/*.+(js|jsx|json|css|scss|ts|tsx|md|mdx)\"",
    "validate": "npm run lint && npm run test && npm run prettier -- --list-different",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "webpack-dev-server --open --config=configs/webpack.dev.js --hot",
    "build": "webpack --mode production --config=configs/webpack.prod.js"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix --max-warnings 2",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "dependencies": {
    "@hot-loader/react-dom": "^16.11.0",
    "lodash": "^4.17.21",
    "normalize.css": "^8.0.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-hot-loader": "^4.12.19",
    "react-redux": "^7.1.3",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "redux-saga": "^1.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@storybook/addon-info": "^5.3.13",
    "@storybook/react": "^5.3.13",
    "@types/git-revision-webpack-plugin": "^3.0.0",
    "@types/html-webpack-plugin": "^3.2.2",
    "@types/jest": "^25.1.2",
    "@types/mini-css-extract-plugin": "^0.9.0",
    "@types/node-sass": "^4.11.0",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/react-redux": "^7.1.7",
    "@types/react-router-dom": "^5.1.3",
    "@types/tape": "^4.2.34",
    "@types/webpack": "^4.41.6",
    "@typescript-eslint/eslint-plugin": "^2.19.0",
    "@typescript-eslint/parser": "^2.19.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-preset-react-app": "^9.1.1",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^1.0.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-config-react-app": "^5.2.0",
    "eslint-import-resolver-webpack": "^0.12.1",
    "eslint-plugin-flowtype": "^4.6.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^2.3.0",
    "file-loader": "^5.0.2",
    "git-revision-webpack-plugin": "^3.0.4",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.2.1",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.7",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "prettier": "^1.19.1",
    "react-docgen-typescript-loader": "^3.6.0",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "tape": "^4.13.0",
    "ts-jest": "^25.2.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "repository": {
    "type": "git",
